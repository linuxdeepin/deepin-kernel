-	rtmp_aes_set_key(&aesctx, key, 128);
+	// store the calculated MIC
+	NdisMoveMemory(pMsg->KeyDesc.KeyMic, mic, LEN_KEY_DESC_MIC);
 
-	for(j = 5; j >= 0; j--)
+	os_free_mem(NULL, OutBuffer);
+}
