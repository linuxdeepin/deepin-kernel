+static int rtusb_probe (struct usb_interface *intf,
+						const struct usb_device_id *id)
+{
+	RTMP_ADAPTER *pAd;
+	struct usb_device *dev;
+	int rv;
+
+	dev = interface_to_usbdev(intf);
+	dev = usb_get_dev(dev);
+
+	rv = rt2870_probe(intf, dev, id, &pAd);
+	if (rv != 0)
+		usb_put_dev(dev);
+
+	return rv;
